package asss1;

public class PrinSeries {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num  = 10;
		int n = 1;
		
		for (int i = 0; i < num; i++) {
			
			int ans = 3*n+2;
			if (ans%4==0) {
				
				n++;
			
				continue;
			}else {
				n++;
				System.out.println(ans);
				
			}
			
			
		}
	}

}
